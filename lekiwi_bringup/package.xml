<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>lekiwi_bringup</name>
  <version>0.0.1</version>
  <description>Launch and system startup for LeKiwi robot</description>
  <maintainer email="adityakamath@live.com">adityakamath</maintainer>
  <license>Apache-2.0</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <!-- Runtime dependencies for launch files -->
  <exec_depend>lekiwi_base_control</exec_depend>
  <exec_depend>camera_ros</exec_depend>
  <exec_depend>ldlidar_ros2</exec_depend>

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>

